<h1>写真一覧</h1>
<% if @photos.present? %>
  <% @photos.each do |photo| %>
    <% if photo.image.attached? %>
      <%= photo.title %>
      <%=image_tag photo.image %>
      <br>
    <% end %>
  <% end %>
<% end %>
<br>
<%= link_to "myTweetAppと接続", @my_tweet_app_url %>
<br>
<%= link_to "新規投稿", new_photo_path %>
<br>
<%= link_to "ログアウト", logout_path %>
